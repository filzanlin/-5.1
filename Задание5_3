abstract class BosTaurus {
    private int weight;

    protected BosTaurus(int weight) {
        this.weight = weight;
    }

    public int getWeight() {
        return weight;
    }

    public abstract void produce();
}

class Cow extends BosTaurus {
    public Cow(int weight) {
        super(weight);
    }

    @Override
    public void produce() {
        System.out.println("Я даю молоко");
    }
}

class Bull extends BosTaurus {
    public Bull(int weight) {
        super(weight);
    }

    @Override
    public void produce() {
        System.out.println("Я даю мясо");
    }
    public static void main(String[] args) {
        Cow cow = new Cow(500);
        Bull bull = new Bull(800);

        System.out.println("Вес коровы: " + cow.getWeight() + " кг");
        cow.produce();

        System.out.println("Вес быка: " + bull.getWeight() + " кг");
        bull.produce();
    }
}
