import java.util.Random;

class Main {
    public static void main(String[] args) {
        String[] applianes = {"Стиральная машина", "Сушильная машина", "Холодильник",
                "Утюг", "Посудомоечная машина", "Микроволновая печь", "Варочная поверхность",
                "Духовой шкаф", "Блендер", "Миксер"};
    int[] amount = new int[10];
        Random random = new Random();

        for (int i = 0; i < amount.length; i++) {
            amount[i] = random.nextInt(11);
        }
        for (int i = 0; i<applianes.length; i++) {
            System.out.println("Наименование: " + applianes[i] + ". Количество: " + amount[i]);
        }
    }
}
