public class Main {
    public static void main(String[] args) {
        int streetNumber = 7;
        int houseNumber = 4;

        for (int i = 1; i <= 7; i++) {
            for (int j = 1; j <= 4; j++) {
                System.out.println("Улица " + i + " дом " + j + " - подарок получен.");
            }
        }
    }
}
